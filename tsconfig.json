{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": true,
    "pretty": true,
    "module": "CommonJS",
    "target": "es5",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "built"
  },
  "exclude": ["built", "node_modules", "examples", "spec"]
}
